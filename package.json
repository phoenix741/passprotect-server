{
  "name": "passprotect-server-2",
  "version": "2.0.0",
  "description": "Server used to keep password of the user in a secure stor",
  "author": "Ulrich Van Den Hekke <ulrich.vdh@shadoware.org>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@graphql-inspector/cli": "^1.21.0",
    "@hapi/joi": "^15.1.0",
    "@nestjs/common": "^6.3.2",
    "@nestjs/core": "^6.3.2",
    "@nestjs/graphql": "^6.2.4",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.3.2",
    "apollo-server-express": "^2.6.7",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.1",
    "graphql-tools": "^4.0.5",
    "mongoose": "^5.6.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "type-graphql": "^0.17.4",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.2",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/joi": "^14.3.3",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.10",
    "@types/supertest": "^2.0.5",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "mockingoose": "^2.13.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../test/jest-setup-file.ts"
    ]
  }
}
