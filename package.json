{
  "name": "passprotect-server",
  "version": "1.2.1",
  "description": "Server used to keep password of the user in a secure store",
  "licence": "MIT",
  "apidoc": {
    "title": "Passprotect REST API documentation",
    "url": "http://localhost:3000",
    "template": {
      "withCompare": false,
      "withGenerator": true
    }
  },
  "private": true,
  "scripts": {
    "start": "babel-node app",
    "compile": "babel ./server --source-maps-inline -d ./dist/server && babel app.js --source-maps-inline -d ./dist/",
    "doc": "apidoc -i server -o doc",
    "eslint": "eslint ./client/scripts ./command ./server karma.conf.js webpack.config.js app.js command.js",
    "lint": "npm run eslint",
    "build": "webpack --progress --profile --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --display-error-details --display-cached",
    "clean": "rimraf dist coverage reports",
    "webpack:server": "webpack-dev-server --hot --colors --progress"
  },
  "main": "",
  "engines": {
    "node": ">= 6.2.1"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "backbone-session": "^1.0.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "~1.17.1",
    "config": "^1.26.1",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.6",
    "errorhandler": "^1.5.0",
    "express": "~4.15.2",
    "express-promise-router": "^1.1.1",
    "graphql": "^0.9.6",
    "graphql-server-express": "^0.7.2",
    "graphql-subscriptions": "^0.3.1",
    "graphql-tools": "^0.11.0",
    "i18next": "^8.1.0",
    "i18next-express-middleware": "^1.0.5",
    "i18next-node-fs-backend": "^1.0.0",
    "jsonwebtoken": "^7.4.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "moment": "^2.18.1",
    "mongodb": "~2.2.26",
    "morgan": "^1.8.1",
    "morgan-debug": "^2.0.0",
    "passport": "^0.3.2",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^2.2.1",
    "permission": "^1.0.4",
    "serve-favicon": "~2.4.2",
    "stream-to-promise": "^2.2.0",
    "subscriptions-transport-ws": "^0.6.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "autoprefixer": "^7.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "backbone": "^1.3.3",
    "backbone-deep-model": "^0.11.0",
    "backbone-validation": "^0.11.5",
    "backbone.marionette": "^3.3.1",
    "backbone.stickit": "^0.9.2",
