  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "DEBUG=App:* node -r @std/esm server/app",
    "start:debug": "DEBUG=App:* node --inspect -r @std/esm server/app",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "unit:server": "nyc -e mjs --reporter=text --reporter=text-summary mocha -r @std/esm --recursive test/**/*.mjs",
    "e2e": "concurrently -k -s first \"node -r @std/esm server/app\" \"node test/e2e/runner.js\"",
    "e2e:docker": "concurrently -k -s first \"node -r @std/esm server/app\" \"node test/e2e/runner.js --config test/e2e/nightwatch.docker.conf.js\"",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.mjs,.vue client server test/unit/specs test/e2e/specs"
  },
