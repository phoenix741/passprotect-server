    "postcss-loader": "^2.0.1",
    "pug": "^2.0.0-rc.1",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
